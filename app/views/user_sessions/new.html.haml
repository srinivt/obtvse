%h1.login Login

= form_for @user_session, :url => {:action => "create"} do |f|
  %div
    = f.label :login
    = f.text_field :login
  %div
    = f.label :password
    = f.password_field :password
  %div.check
    = f.check_box :remember_me
    = f.label :remember_me
  %div.submit
    = f.submit "Login"

